{% extends 'magasin.html.twig' %}



{% block body %}
	<div class="text-center">
		<h1>Modifier le problème "{{probleme.NomProbleme}}"</h1>
		<a class="btn btn-secondary" href="{{ path('probleme_index') }}">Retour</a>
		<br/>
		<br/>
		<form name="probleme" method="post">
			<p>Nom du problème</p>
			{{ form_widget(form.NomProbleme) }}
			<br/><br/>
			<button class="btn btn-success">{{ button_label|default('Modifier') }}</button>
			{{ form_row(form._token) }}
		</form>
	</div>
{% endblock %}